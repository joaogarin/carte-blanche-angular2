    "angular2",
    "carteblanche"
  ],
  "main": "dist/plugin.js",
  "author": "joaogarin <joao.garin@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/joaogarin/carte-blanche-angular2.git"
  },
  "bugs": {
    "url": "https://github.com/joaogarin/carte-blanche-angular2/issues"
  },
  "homepage": "https://github.com/joaogarin/carte-blanche-angular2",
  "dependencies": {
    "@angular/common": "2.0.0-rc.6",
    "@angular/compiler": "2.0.0-rc.6",
    "@angular/core": "2.0.0-rc.6",
    "@angular/platform-browser": "2.0.0-rc.6",
    "@angular/platform-browser-dynamic": "2.0.0-rc.6",
    "@angular/platform-server": "2.0.0-rc.6",
    "@angular/http": "2.0.0-rc.6",
    "@angular/forms": "^2.0.0-rc.6",
    "core-js": "^2.2.2",
    "faker": "^3.1.0",
    "lodash": "^4.13.1",
    "mkdirp": "^0.5.1",
    "path": "^0.12.7",
    "react": "^15.0.1",
    "rxjs": "5.0.0-beta.11",
    "zone.js": "^0.6.12",
    "chokidar": "^1.5.1",
    "cors": "^2.7.1",
    "express": "^4.13.4",
    "socket.io": "^1.4.6",
    "socket.io-client": "^1.4.6"
  },
  "devDependencies": {
    "awesome-typescript-loader": "^1.1.1",
    "babel-cli": "^6.9.0",
    "babel-preset-es2015": "",
    "babel-preset-react": "",
    "babel-preset-stage-0": "",
    "codelyzer": "0.0.28",
    "commitizen": "^2.8.6",
    "cz-conventional-changelog": "^1.2.0",
    "es6-promise-loader": "^1.0.1",
    "es6-shim": "^0.35.0",
    "es7-reflect-metadata": "^1.6.0",
    "istanbul-instrumenter-loader": "^0.2.0",
    "jasmine-core": "^2.4.1",
    "karma": "^1.1.1",
    "karma-coverage": "^1.1.0",
    "karma-jasmine": "^1.0.2",
    "karma-mocha-reporter": "^2.0.4",
    "karma-phantomjs-launcher": "^1.0.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "semantic-release": "^4.3.5",
    "source-map-loader": "^0.1.5",
    "ts-helper": "0.0.1",
    "ts-helpers": "^1.1.1",
    "ts-node": "^0.7.0",
    "tsc": "^1.20150623.0",
    "tsconfig-lint": "^0.6.0",
    "tslint": "^3.5.0",
    "tslint-loader": "^2.1.5",
    "typedoc": "^0.4.0",
    "typescript": "~1.8.9",
    "typings": "^1.3.2",
    "webpack": "^1.13.1",
    "webpack-merge": "^0.14.0"
  },
  "peerDependencies": {
    "carte-blanche": "^0.3.1",
    "webpack": ">=1 <3"
  },
  "scripts": {
    "commit": "git-cz",
    "clean-install": "rm -rf node_modules && rm -rf typings && npm install",
    "build-plugin": "BABEL_DISABLE_CACHE=1 BABEL_ENV=production babel --ignore=node_modules --out-dir='dist' plugin.js typedocparser.js server/server.js server/run.js server/utils/getAbsoluteVariationPath.js",
    "build-frontend": "webpack --config webpack.config.js --progress --profile --colors --display-error-details --display-cached",
    "frontend": "NODE_ENV=production webpack",
    "build": "npm run build-plugin && npm run frontend",
    "webpack-test": "webpack --config webpack.test.js --progress --profile --colors --display-error-details --display-cached",
    "test": "karma start",
    "typings-install": "typings install",
    "postinstall": "npm run typings-install",
    "prepublish": "npm run build",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "release": {
    "debug": false,
    "branch": "master"
  },
  "czConfig": {
    "path":"node_modules/cz-conventional-changelog"
  }
}
